{	// Markdown.html
	"img-center":{
		"prefix": "div",
		"body": "<div align=center> <img src=\"$0\"> </div>",
	},
	"p-center":{
		"prefix": "p",
		"body": "<p align=\"center\">$0</p>",
	},

	// annotation
	"fixme":{
		"prefix": "fix",
		"body": "// FIXME: ",
	},
	"todo":{
		"prefix": "todo",
		"body": "// TODO: ",
	},
	"warning":{
		"prefix": "warning",
		"body": "// WARNING: ",
	},
}