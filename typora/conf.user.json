{
  "defaultFontFamily": {
    "standard": null, 
    "serif": null, 
    "sansSerif": null,
    "monospace": null
  },
  "autoHideMenuBar": false, 

  
  "searchService": [
    ["Search with Google", "https://google.com/search?q=%s"]
