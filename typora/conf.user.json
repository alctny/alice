    "Rename": "F2",
    "Outline": "Ctrl+Shift+l",
    "File Tree": "Ctrl+Alt+l"
  },
  "editor":{
    "fontSize": 20
   },
  "monocolorEmoji": false,
  "maxFetchCountOnFileList": 500,
  "flags": [] 
}
